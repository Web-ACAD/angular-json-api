{
	"name": "@webacad/angular-json-api",
	"description": "Angular module for working with data from standardized json api",
	"version": "0.0.3",
	"keywords": [
		"angular",
		"json",
		"api"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git@github.com:Web-ACAD/angular-json-api.git"
	},
	"main": "./lib/index.js",
	"author": {
		"name": "David Kudera",
		"email": "kudera.d@gmail.com",
		"url": "https://www.davidkudera.com"
	},
	"peerDependencies": {
		"@angular/core": ">=5.0.0"
	},
	"devDependencies": {
		"@types/chai": "^4.1.2",
		"@angular/compiler": ">=5.0.0",
		"@angular/compiler-cli": ">=5.0.0",
		"@types/mocha": "^5.0.0",
		"@types/node": "8.0.19",
		"chai": "^4.1.2",
		"mocha": "^5.0.5",
		"ng-packagr": "^2.2.0",
		"npm-install-peers": "^1.2.1",
		"ts-node": "^5.0.1",
		"tslint": "^5.9.1",
		"typescript": "^2.7.2"
	},
	"scripts": {
		"build": "ng-packagr -p ng-package.json",
		"test": "mocha -r ts-node/register test/tests/**/*.ts",
